{
  "compilerOptions": {
    "outDir": "./public",
    "target": "ES2020",
    "module": "CommonJS",
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "jsx": "react",
    "jsxFactory": "h",
    "esModuleInterop": true
  },
  "include": [
    "./src/**/*.tsx",
    "./src/**/*.ts"
  ]
}
