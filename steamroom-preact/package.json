{
  "private": true,
  "name": "steamroom",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "serve": "sirv build --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/blk-design-system-pro-react.scss src/assets/css/blk-design-system-pro-react.css",
    "minify-sass": "node-sass src/assets/scss/blk-design-system-pro-react.scss src/assets/css/blk-design-system-pro-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/blk-design-system-pro-react.scss src/assets/css/blk-design-system-pro-react.css --source-map true"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "preact-cli": "^3.0.0-rc.6",
    "preact-render-spy": "^1.2.1",
    "sirv-cli": "^0.4.5",
    "@types/googlemaps": "3.30.20",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.8.17",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "chart.js": "2.8.0",
    "headroom.js": "0.9.4",
    "moment": "2.24.0",
    "node-pre-gyp": "^0.14.0",
    "node-sass": "^4.13.1",
    "nouislider": "13.1.5",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "react-bootstrap-switch": "15.5.3",
    "react-chartjs-2": "2.7.6",
    "react-datetime": "2.16.3",
    "react-dom": "16.8.6",
    "react-google-maps": "9.4.5",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "react-scripts": "^3.4.1",
    "react-select": "2.4.3",
    "react-slick": "0.24.0",
    "react-tagsinput": "3.19.0",
    "reactstrap": "8.0.0"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  },
  "main": "index.js",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
